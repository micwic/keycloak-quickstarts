{
  "exclude": [
    "/node_modules/"
  ],
  "compilerOptions": {
    "rootDir": ".",
    "jsx": "react",
    "module": "esNext",
    "moduleResolution": "node",
    "outDir": "./dist",
    "preserveConstEnums": true,
    "strict": true,
    "strictFunctionTypes": false,
    "target": "es5",
    "sourceMap": true,
    "importHelpers": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  }
}